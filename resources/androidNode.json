{
	"capabilities":
	[
		{
			"platform": "Android",
			"platformName": "Android",
			"appium:automationName": "UiAutomator2",
			"maxInstances": "1"
		}
	],
	"configuration":
		{
			"proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
			"cleanUpCycle": 2000,
			"timeout": 30000,
			"maxSession": 1,
			"register": true,
			"registerCycle": 5000,
			"nodeStatusCheckTimeout": 5000,
			"role": "node",
			"unregisterIfStillDownAfter": 60000,
			"downPollingLimit": 2,
			"debug": true,
			"servlets": [],
			"withoutServlets": [],
			"custom": {},
			"hubPort": 4444,
			"hubHost": "0.0.0.0",
			"hubProtocol": "http"
		}
}